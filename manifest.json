{
  "name": "youtube scroll",
  "version": "1.0",
  "description": "scroll related videos and comments on the youtube pages.",
  "permissions": [],
  "icons": {
    "16": "icon/icon_16.png",
    "32": "icon/icon_32.png",
    "48": "icon/icon_48.png",
    "128": "icon/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "page_action": {},
  "manifest_version": 2
}
